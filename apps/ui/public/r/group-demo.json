{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "group-demo",
  "type": "registry:example",
  "description": "Group example",
  "registryDependencies": [
    "@coss/group",
    "@coss/button"
  ],
  "files": [
    {
      "path": "registry/default/examples/group-demo.tsx",
      "content": "import {\n  CopyIcon,\n  DownloadIcon,\n  EllipsisIcon,\n  FilesIcon,\n  FilmIcon,\n  FilterIcon,\n  SearchIcon,\n  ZoomInIcon,\n  ZoomOutIcon,\n} from \"lucide-react\"\n\nimport { Button } from \"@/registry/default/ui/button\"\nimport { Group, GroupSeparator, GroupText } from \"@/registry/default/ui/group\"\nimport { Input } from \"@/registry/default/ui/input\"\nimport { Label } from \"@/registry/default/ui/label\"\nimport {\n  Select,\n  SelectItem,\n  SelectPopup,\n  SelectTrigger,\n  SelectValue,\n} from \"@/registry/default/ui/select\"\n\nconst items = [\n  { label: \"Select framework\", value: null },\n  { label: \"Next.js\", value: \"next\" },\n  { label: \"Vite\", value: \"vite\" },\n  { label: \"Astro\", value: \"astro\" },\n]\n\nexport default function GroupDemo() {\n  return (\n    <div className=\"flex flex-col gap-4\">\n      <Group>\n        <Button variant=\"outline\">\n          <FilesIcon />\n          Files\n        </Button>\n        <GroupSeparator />\n        <Button variant=\"outline\">\n          <FilmIcon />\n          Media\n        </Button>\n        <GroupSeparator />\n        <Button variant=\"outline\" size=\"icon\" aria-label=\"Menu\">\n          <EllipsisIcon />\n        </Button>\n      </Group>\n\n      <Group>\n        <Button>\n          <FilesIcon />\n          Files\n        </Button>\n        <GroupSeparator className=\"bg-primary/72\" />\n        <Button>\n          <FilmIcon />\n          Media\n        </Button>\n        <GroupSeparator className=\"bg-primary/72\" />\n        <Button size=\"icon\" aria-label=\"Menu\">\n          <EllipsisIcon />\n        </Button>\n      </Group>\n\n      <Group>\n        <Input type=\"text\" defaultValue=\"https://coss.com\" />\n        <GroupSeparator />\n        <Button variant=\"outline\" size=\"icon\" aria-label=\"Copy\">\n          <CopyIcon />\n        </Button>\n      </Group>\n\n      <Group>\n        <Button variant=\"outline\" size=\"icon\" aria-label=\"Filter\">\n          <FilterIcon />\n        </Button>\n        <GroupSeparator />\n        <Input\n          type=\"text\"\n          defaultValue=\"https://coss.com\"\n          aria-label=\"Search\"\n        />\n        <GroupSeparator />\n        <Button variant=\"outline\">\n          <SearchIcon />\n          Button\n        </Button>\n        <GroupSeparator />\n        <Input type=\"email\" defaultValue=\"Enter email\" aria-label=\"Email\" />\n        <GroupSeparator />\n        <Select items={items} defaultValue=\"next\">\n          <SelectTrigger>\n            <SelectValue />\n          </SelectTrigger>\n          <SelectPopup>\n            {items.map(({ label, value }) => (\n              <SelectItem key={value} value={value}>\n                {label}\n              </SelectItem>\n            ))}\n          </SelectPopup>\n        </Select>\n        <GroupSeparator />\n        <Button variant=\"outline\" size=\"icon\" aria-label=\"Download\">\n          <DownloadIcon />\n        </Button>\n      </Group>\n\n      <Group>\n        <GroupText render={<Label htmlFor=\"domain\" aria-label=\"Domain\" />}>\n          https://\n        </GroupText>\n        <GroupSeparator />\n        <Input\n          id=\"domain\"\n          type=\"text\"\n          defaultValue=\"coss.com\"\n          aria-label=\"Domain\"\n        />\n      </Group>\n\n      <Group orientation=\"vertical\">\n        <Button variant=\"outline\" size=\"icon\" aria-label=\"Zoom in\">\n          <ZoomInIcon />\n        </Button>\n        <GroupSeparator orientation=\"horizontal\" />\n        <Button variant=\"outline\" size=\"icon\" aria-label=\"Zoom Out\">\n          <ZoomOutIcon />\n        </Button>\n      </Group>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ],
  "categories": [
    "group"
  ]
}